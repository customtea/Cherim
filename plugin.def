[Plugin]
Id=net.customtea.cherim
Name=Cherim
Description=Sakura Editor wrap Vim (like) KeyBind
Type=wsh
Author=customtea
Version=1.0.0
Url=https://github.com/customtea/cherim

[Wsh]
UseCache=1

[Command]
C[1]=keys/esc.js
C[1].Label=key_esc
C[2]=keys/h.js
C[2].Label=key_h
C[3]=keys/j.js
C[3].Label=key_j
C[4]=keys/k.js
C[4].Label=key_k
C[5]=keys/l.js
C[5].Label=key_l
C[6]=keys/y.js
C[6].Label=key_y


[Plug]
EditorStart=start.js
SmartIndent=main.js
SmartIndent.Label=cherim

