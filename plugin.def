[Plugin]
Id=net.customtea.cherim
Name=Cherim
Description=Sakura Editor wrap Vim (like) KeyBind
Type=wsh
Author=customtea
Version=1.8.2
Url=https://github.com/customtea/cherim

[Wsh]
UseCache=1

[Command]
C[1]=keys/debug.js
C[1].Label=Cherim_Debug
C[2]=keys/Esc.js
C[2].Label=Cherim_Esc
C[3]=keys/h.js
C[3].Label=Cherim_h
C[4]=keys/j.js
C[4].Label=Cherim_j
C[5]=keys/k.js
C[5].Label=Cherim_k
C[6]=keys/l.js
C[6].Label=Cherim_l
C[7]=keys/y.js
C[7].Label=Cherim_y
C[8]=keys/Tab.js
C[8].Label=Cherim_Tab
C[9]=keys/S-Tab.js
C[9].Label=Cherim_S-Tab
C[10]=keys/G.js
C[10].Label=Cherim_G
C[11]=keys/C-v.js
C[11].Label=Cherim_C-v
C[12]=keys/C-r.js
C[12].Label=Cherim_C-r


[Plug]
DocumentOpen=start.js
SmartIndent=main.js
SmartIndent.Label=cherim

